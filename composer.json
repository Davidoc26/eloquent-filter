{
  "name": "davidoc26/eloquent-filter",
  "description": "Simple filter system for building queries",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "David Eritsyan",
      "email": "dav.eritsyan@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Davidoc26\\EloquentFilter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^8.0 || ^9.0",
    "illuminate/container": "^8.0 || ^9.0",
    "illuminate/database": "^8.0 || ^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0 || ^v7.0",
    "phpunit/phpunit": "^9.5",
    "ext-pdo_sqlite": "*",
    "vimeo/psalm": "^4.17"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Davidoc26\\EloquentFilter\\ServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": false
    }
  }
}
